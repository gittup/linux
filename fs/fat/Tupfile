include_rules
modname = fat
#
# Makefile for the Linux fat filesystem support.
#

vfat-@(VFAT_FS) += namei_vfat.c
msdos-@(MSDOS_FS) += namei_msdos.c

fat-y := cache.c dir.c fatent.c file.c inode.c misc.c
obj-@(FAT_FS) += $(fat-y)
obj-@(FAT_FS) += $(vfat-y)
obj-@(FAT_FS) += $(msdos-y)
include $(LINUX_ROOT)/linux.tup
