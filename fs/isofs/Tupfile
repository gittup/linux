include_rules
#
# Tupfile for the Linux isofs filesystem routines.
#

obj-$(CONFIG_ISO9660_FS) += isofs.o

isofs-objs-y 			:= namei.c inode.c dir.c util.c rock.c export.c
isofs-objs-$(CONFIG_JOLIET)	+= joliet.c
isofs-objs-$(CONFIG_ZISOFS)	+= compress.c
isofs-objs			:= $(isofs-objs-y)
include $(LINUX_ROOT)/linux2.tup
