include_rules
#
# Tupfile for the Linux proc filesystem routines.
#

obj-$(CONFIG_PROC_FS) += proc.o

proc-y			:= nommu.c task_nommu.c
proc-$(CONFIG_MMU)	:= mmu.c task_mmu.c

proc-y       += inode.c root.c base.c generic.c array.c \
		proc_tty.c
proc-y	+= cmdline.c
proc-y	+= cpuinfo.c
proc-y	+= devices.c
proc-y	+= interrupts.c
proc-y	+= loadavg.c
proc-y	+= meminfo.c
proc-y	+= stat.c
proc-y	+= uptime.c
proc-y	+= version.c
proc-y	+= softirqs.c
proc-$(CONFIG_PROC_SYSCTL)	+= proc_sysctl.c
proc-$(CONFIG_NET)		+= proc_net.c
proc-$(CONFIG_PROC_KCORE)	+= kcore.c
proc-$(CONFIG_PROC_VMCORE)	+= vmcore.c
proc-$(CONFIG_PROC_DEVICETREE)	+= proc_devtree.c
proc-$(CONFIG_PRINTK)	+= kmsg.c
proc-$(CONFIG_PROC_PAGE_MONITOR)	+= page.c
include $(LINUX_ROOT)/linux2.tup
