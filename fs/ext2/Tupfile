include_rules
#
# Tupfile for the linux ext2-filesystem routines.
#

obj-$(CONFIG_EXT2_FS) += ext2.o

ext2-y := balloc.c dir.c file.c ialloc.c inode.c \
	  ioctl.c namei.c super.c symlink.c

ext2-$(CONFIG_EXT2_FS_XATTR)	 += xattr.c xattr_user.c xattr_trusted.c
ext2-$(CONFIG_EXT2_FS_POSIX_ACL) += acl.c
ext2-$(CONFIG_EXT2_FS_SECURITY)	 += xattr_security.c
ext2-$(CONFIG_EXT2_FS_XIP)	 += xip.c
include $(LINUX_ROOT)/linux2.tup
